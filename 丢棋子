#include<iostream>
using namespace std;
int Process1(int K,int N)
{
    if(N==0) return 0;
    if(K==1) return N;
    int mi=65535999;
    for(int i=1;i<(N+1);i++)
    {
            int ma=max(Process1(K-1,i-1),Process1(K,N-i));
            mi=min(mi,ma);
    }
    return mi+1;
}
int Solution(int K,int N)
{
    if(K<1||N<1)
    {
        return 0;
    }
    return Process(K,N);

}

int main()
{
    int KK=1;//K个棋子
    int NN=10;//N个楼层
    int res = Solution(KK,NN);
    cout<<res;
}
