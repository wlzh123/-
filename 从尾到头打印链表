#include<iostream>
using namespace std;

struct ListNode{
    int data;
    ListNode *next;
};
//可以用栈 就可以用递归实现
void PrintListNode(ListNode *Head){
    if(Head!=NULL){
        if(Head->next!=NULL)
            PrintListNode(Head->next);
        cout<<Head->data<<endl;
    }

}
int main(){

    ListNode *head=new ListNode;
    head->data=0;
    head->next=NULL;
    ListNode *tmp=head;
    for(int i=1;i<5;i++)
    {
        ListNode *node=new ListNode;
        node->data=i;
        tmp->next=node;
        tmp=tmp->next;
        tmp->next=NULL;
    }

    PrintListNode(head);
}
