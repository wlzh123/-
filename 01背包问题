#include<iostream>
using namespace std;
int main(){
    int v[]={8,10,6,3,7,2};
    int w[]={4,6 ,2,2,5,1};
    int dp[7][13];
    for(int i=0;i<13;i++)
        dp[0][i]=0;
    for(int i=0;i<7;i++)
        dp[i][0]=0;
    for(int i=1;i<7;i++){
        for(int j=1;j<13;j++){
                if(j>w[i])//第i有两种选择 背还是不背
            dp[i][j]=max(dp[i-1][j],dp[i-1][j-w[i]]+v[i]);
        else
            dp[i][j]=dp[i-1][j];//没得选
        }
    }
    for(int i=1;i<7;i++)
    {
        for(int j=1;j<13;j++)
        {
            cout<<dp[i][j]<<' ';
        }
        cout<<endl;
    }


    return 0;
}


