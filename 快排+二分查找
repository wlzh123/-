#include<iostream>
using namespace std;
int Binary_Serch(int a[],int low,int high ,int n){

  while(low<=high){
    int mid=(high+low)/2;
    if(a[mid]>n)
        high=mid-1;
    else if(a[mid]<n)
        low=mid+1;
    else
        return mid;
  }

     return -1;
}
void   Qsort(int arr[],int low,int high)
{
    if(low>high) return ;
    int first=low;
    int last=high;
    int key=arr[first];
    while(first<last){
        while(arr[last]>=key&&first<last){
            --last;
        }
        arr[first]=arr[last];
        while(arr[first]<=key&&first<last){
            ++first;
        }
        arr[last]=arr[first];
    }
    arr[first]=key;
    Qsort(arr,low,first-1);
    Qsort(arr,last+1,high);

}

int main(){
    int ss[]={1,2,322,4,5,6,7,8};
    Qsort(ss,0,6);
    for(int i=0;i<=6;++i)
        cout<<ss[i]<<' ';
    cout<<endl;
    int x= Binary_Serch(ss,0,6,4);
    cout<<x;
}
