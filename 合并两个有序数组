#include<iostream>
using namespace std;
void merge1(int arr1[],int m,int arr2[],int n,int arr3[]){
    int i=0;
    int j=0;
    int k=0;
    while(i<m&&j<n){
        if(arr1[i]<arr2[j])
            arr3[k++]=arr1[i++];
         else
            arr3[k++]=arr2[j++];
    }
    while(i<m)
        arr3[k++]=arr1[i++];
    while(j<n)
        arr3[k++]=arr2[j++];
}
int main(){
    int ss1[]={1,2,3,4,5,6};
    int ss2[]={7,8,9};
    int ss[9];
    merge1(ss1,6,ss2,3,ss);
    for(int i=0;i<9;i++)
        cout<<ss[i];
}

