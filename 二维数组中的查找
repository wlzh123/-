#include<iostream>
using namespace std;
bool func( int arr[][4],int m,int num){
    bool res=false;
    int n=sizeof(arr[0])/sizeof(int);//m行 n列
    int m1=m-1;
    int n1=0;//从左下角开始算起
    while(m1>=0&&n1<n){
        if(arr[m1][n1]==num){
            res=true;
            break;
        }
        else if(arr[m1][n1]>num){

            --m1;
        }
        else{
            ++n1;
        }
    }

    return res;
}

int main(){

int ar[4][4]={{1,2,3,4},
             {4,5,6,7},
             {5,6,7,8},
             {6,7,8,9}};
bool res=func(ar,4,100);
cout<<res<<endl;
}
