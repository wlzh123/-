#include<iostream>
#include<math.h>
using namespace std;
 bool Func(int num)
 {
    int n=abs(num);
    int help=1;
    while((n/help)>=10)
    {
        help*=10;
    };
    while(n!=0)
    {
        if((n/help)!=(n%10))
            {
                return false;
            }
        n=(n%help)/10;
        help/=100;
    }

    return true;
 }
int main()
{
 int num=1234554321;
 bool res= Func(num);
 cout<<res<<endl;
 return 0;
}
