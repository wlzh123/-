/////////去除链表中的重复结点
/////////例：1-2-2-3-3-3-4-5-5-5-5-5-5  转换为1-2-3-4-5
#include<iostream>
using namespace std;
struct ListNode{
  int val;
  struct ListNode *next;
};
void Erase_Node(ListNode* pHead){
   if(pHead==NULL) return ;
           ListNode* p1=pHead;
           ListNode* p2=p1->next;
       while(p2!=NULL){
          if(p1->val==p2->val){
              p1->next=p2->next;
              p2=p2->next;
          }
          else{
              p1=p2;
              p2= p2->next;
          }

       }
       // return pHead->next;
}

int main(){

    ListNode* head=new ListNode;
    ListNode* ss=head;
    ListNode* Head=head;
    for(int i=0;i<10;++i){
        ListNode* temp=new ListNode;
        temp->val=2;
        ss->next=temp;
        ss=temp;
        temp->next=NULL;
    }

  Erase_Node(Head);
  Head=Head->next;

   while(Head!=NULL){
      cout<<Head->val;
      Head=Head->next;
   }

}
