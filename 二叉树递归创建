#include<iostream>
using namespace std;
///定义节点 
typedef struct Node{
  int a;
  struct Node *left;
  struct Node *right;
}TreeNode,*PNode;

CreatTree(PNode &T){

    char c;
    cin>>c;
    if('*'==c)
      T=NULL;
    else{
        T=new TreeNode;
        T->a=c;
        CreatTree(T->left);
        CreatTree(T->right);
    }
}

int main(){

   PNode tree;
   CreatTree(tree);
   return 0;
 }
