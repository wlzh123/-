#include<iostream>
using namespace std;

int Get_sum(int arr[],int aim,int length)
{
    int dp[length][aim+1];//={65535};
    for(int i=0;i<=aim;i++)
    {
        dp[0][i]=65535;
        if(i-arr[0]>=0&&i%arr[0]==0)
            dp[0][i]=i/arr[0];
          //cout<<dp[0][i]<<" ";
    }
    int left=0;
    for(int i=1;i<length;i++)
    {
        for(int j=1;j<=aim;j++)
        {
            left=65535;
            if( j-arr[i]>=0&&dp[i][j-arr[i]]!=65535)
            {
               left=dp[i][j-arr[i]]+1;
            }
            if(j==arr[i]) left=1;
            dp[i][j]=min(dp[i-1][j],left);

        }
    }
    return dp[length-1][aim]==65535?-1:dp[length-1][aim];
}
int main()
{
    int arr[]={2,3,4,10};
    int aim=4;
    int len=sizeof(arr)/sizeof(int);
    int res=Get_sum(arr,aim,len);
    cout<<res<<endl;
}
